{
  "name": "chatroom-client",
  "version": "1.0.0",
  "private": true,
  "bin": "combined-server.js",
  "pkg": {
    "assets": [
      "build/**/*"
    ],
    "targets": [
      "node16-win-x64",
      "node16-macos-x64",
      "node16-linux-x64"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
